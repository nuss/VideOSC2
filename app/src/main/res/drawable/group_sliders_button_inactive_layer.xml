<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_activated="false"
        android:state_enabled="true"
        android:state_pressed="false">

        <shape
            android:shape="oval">
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"/>
            <solid
                android:color="#ff336699"/>
            <stroke
                android:width="2dp"
                android:color="#eecccccc"/>
        </shape>

    </item>
    <item
        android:drawable="@drawable/group_sliders"/>
</layer-list>