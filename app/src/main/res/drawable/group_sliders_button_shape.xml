<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:state_activated="false"
		android:state_enabled="true"
		android:state_pressed="false">

		<shape
			android:shape="oval">
			<padding
				android:bottom="10dp"
				android:left="10dp"
				android:right="10dp"
				android:top="10dp"/>
			<solid
				android:color="#cc000000"/>
			<stroke
				android:width="2dp"
				android:color="#eecccccc"/>
		</shape>

	</item>
	<item>

		<shape
			android:shape="oval">
			<padding
				android:bottom="10dp"
				android:left="10dp"
				android:right="10dp"
				android:top="10dp"/>
			<solid
				android:color="#cc336699"/>
			<stroke
				android:width="2dp"
				android:color="#eeffffff"/>
		</shape>

	</item>
</selector>
