<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"/>
            <solid
                android:color="#C0E182"/>
            <stroke
                android:width="2dp"
                android:color="#ff52851F"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/group_sliders_active"/>
</layer-list>